#site_scons
import boost
import unit_test
import snet

Import('env_base')

static_libs = [
	boost.static_library('date_time'),
	boost.static_library('filesystem'),
	boost.static_library('system'),
	boost.static_library('thread')
]

env_base.Program('http',
	['main.cpp', 'http.cpp', snet.static_library()] + static_libs
)
